<Window x:Class="Labs.OldLabs.Lab4Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labs.OldLabs"
        mc:Ignorable="d"
        Title="Lab4Window" Height="450" Width="800">
    <Grid>
        <Label Content="Первая строка" HorizontalAlignment="Left" Margin="250,170,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="34" Width="90"/>
        <TextBox x:Name="inputAString" HorizontalAlignment="Left" Height="23" Margin="345,170,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="158"/>


        <Label Content="Вторая строка" HorizontalAlignment="Left" Margin="252,209,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.647,0.659" Height="27"/>
        <TextBox x:Name="inputBString" HorizontalAlignment="Left" Height="23" Margin="345,213,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="158"/>

        <Label Content="" Name="labelResult" HorizontalAlignment="Left" Margin="320,292,0,0" VerticalAlignment="Top" Width="93"/>

        <Button Content="Вычислить" Click="Button_Click_Calculate" HorizontalAlignment="Left" Margin="320,254,0,0" VerticalAlignment="Top" Width="93"/>
    </Grid>
</Window>
